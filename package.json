{
  "name": "apprenticevr",
  "version": "1.3.1",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "electron-vite build && electron-builder --win",
    "build:win:x64": "electron-vite build && electron-builder --win --x64",
    "build:win:ia32": "electron-vite build && electron-builder --win --ia32",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:mac:x64": "electron-vite build && electron-builder --mac --x64",
    "build:mac:arm64": "electron-vite build && electron-builder --mac --arm64",
    "build:mac:universal": "electron-vite build && electron-builder --mac --universal",
    "build:linux": "electron-vite build && electron-builder --linux",
    "build:linux:x64": "electron-vite build && electron-builder --linux --x64",
    "build:linux:arm64": "electron-vite build && electron-builder --linux --arm64",
    "build:all": "electron-vite build && electron-builder --win --mac --linux",
    "build:all:x64": "electron-vite build && electron-builder --win --mac --linux --x64",
    "publish": "electron-builder --publish always",
    "tag-version": "VERSION=$(node -p \"require('./package.json').version\") && TAG_NAME=\"v$VERSION\" && (git rev-parse $TAG_NAME >/dev/null 2>&1 || git tag $TAG_NAME) && echo \"Tag $TAG_NAME created or already exists.\""
  },
  "dependencies": {
    "@devicefarmer/adbkit": "^3.3.8",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@fluentui/react-components": "^9.64.0",
    "@fluentui/react-icons": "^2.0.302",
    "@fluentui/react-popover": "^9.10.7",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.9",
    "axios": "^1.9.0",
    "bluebird": "^3.7.2",
    "compare-versions": "^6.1.1",
    "data-uri-to-buffer": "^6.0.2",
    "date-fns": "^4.1.0",
    "electron-log": "^5.4.0",
    "electron-updater": "^6.6.2",
    "execa": "^9.6.0",
    "fetch-blob": "^4.0.0",
    "formdata-polyfill": "^4.0.10",
    "ini": "^5.0.0",
    "match-sorter": "^8.0.2",
    "node-7z": "^3.0.0",
    "node-wget-fetch": "^1.1.3",
    "react-youtube": "^10.1.0",
    "use-disposable": "^1.0.4",
    "when": "^3.7.8",
    "yauzl": "^3.2.0",
    "yt-search": "^2.13.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/bluebird": "^3.5.42",
    "@types/ini": "^4.1.1",
    "@types/node": "^22.15.29",
    "@types/node-7z": "^2.1.10",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/when": "^2.4.41",
    "@types/yt-search": "^2.10.3",
    "@vitejs/plugin-react": "^4.5.1",
    "electron": "^36.3.2",
    "electron-builder": "^26.0.12",
    "electron-vite": "^3.1.0",
    "eslint": "^9.28.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild"
    ]
  }
}
